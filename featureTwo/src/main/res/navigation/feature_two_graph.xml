<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/feature_two_graph"
    app:startDestination="@id/fragmentPager">

    <fragment
        android:id="@+id/fragmentPager"
        android:name="com.example.featchertwo.fragments.FragmentPager"
        android:label="FragmentPager" >
        <action
            android:id="@+id/action_fragmentPager_to_fragmentTab1_1"
            app:destination="@id/fragmentTab1_1" />
        <action
            android:id="@+id/action_fragmentPager_to_fragmentTab2_1"
            app:destination="@id/fragmentTab2_1" />
    </fragment>
    <fragment
        android:id="@+id/fragmentTab1_1"
        android:name="com.example.featchertwo.fragments.FragmentTab1_1"
        android:label="FragmentTab1_1" />
    <fragment
        android:id="@+id/fragmentTab2_1"
        android:name="com.example.featchertwo.fragments.FragmentTab2_1"
        android:label="FragmentTab2_1" />
</navigation>